{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "misinformation-intelligence-platform",
  "version": "1.0.0",
  "description": "AI-Centric Misinformation Intelligence Platform with Multi-Agent Architecture",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "install-all": "npm install && cd client && npm install",
    "setup-ml": "node setup-ml-verification.js",
    "setup-ml:windows": "node setup-windows-ml.js",
    "test-ml": "node test-ml-verification.js",
    "quick-start": "node quick-start.js",
    "quick-start:full": "node quick-start.js --start-server",
    "quick-start:windows": "node setup-windows-ml.js && npm start",
    "setup-only": "node quick-start.js --setup-only",
    "test-only": "node quick-start.js --test-only"
  },
  "keywords": [
    "ai",
    "misinformation",
    "fact-checking",
    "multi-agent"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.1.3",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.12",
    "chromadb": "^1.7.0",
    "compromise": "^14.10.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.0.200",
    "mongoose": "^8.0.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "openai": "^4.20.0",
    "puppeteer": "^19.11.1",
    "sentiment": "^5.0.2",
    "socket.io": "^4.6.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}
